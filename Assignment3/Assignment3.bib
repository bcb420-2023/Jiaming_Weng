	pmcid = {PMC4015626},
	pages = {49--54},
	file = {PubMed Central Full Text PDF:/Users/wengjiaming/Zotero/storage/MMKNWF2L/Signer et al. - 2014 - Haematopoietic stem cells require a highly regulat.pdf:application/pdf},
}

